/* Password Page Modal CSS */
.controls {
  display: block;
  margin: 1em 2em 1em auto;
  text-align: right;
  text-transform: uppercase;
  position: relative;
}

#open-me a { color: {{ settings.password_page_text_color }}; }

.close-me { z-index: 8000; }
.close-me a { color: #000; }

.modalbox {
  position: absolute;
  height: 100%;
  top: -1em;
  bottom: 0;
  left: 0;
  right: 0;
  padding: 0;
  z-index:5000;
}

.overlay {
  position: fixed;
  width: 100%;
  height: 100%;
  max-height: 100%;
  top: 0;
  left: 0;
  background: #fff;
  background: rgba(255,255,255,0.95);
}

.overlay-close {
  width: 150px;
  height: 50px;
  position: absolute;
  right: 10px;
  top: 10px;
  border: 1px solid #000;
  font-size: 14px;
}
.overlay-data {
  opacity: 0;
  visibility: hidden;
  -webkit-transition: opacity 0.5s;
  transition: opacity 0.5s;
  visibility: 0s 0.5s;
  transition: opacity 0.5s, visibility 0s 0.5s;
}
.overlay-open {
  opacity: 1;
  visibility: visible;
  -webkit-transition: opacity 0.5s;
  transition: opacity 0.5s;
}

.inputbox {
  margin: 0px auto;
  max-width: 100%;
  display: table;
  text-align: center;
  position: absolute;
  top: 50%;
  left: 50%;
  margin-right: -50%;
  -ms-transform: translate(-50%, -50%);
  -webkit-transform: translate(-50%, -50%);
  transform: translate(-50%, -50%);
  color: #000;
}

.storefront-password-form label {
  font-size: 0.9em;
  margin: 0 0 1em 0;
}

.storefront-password-form .actions {
  display: inline-block;
}

.storefront-password-form #password {
  width: 80%;
  display: inline-block;
}

#owner {
  font-size: 0.9em;
  margin-top: -1em;
  opacity: 0.8;
}

.pass-close {
  color: #000 !important;
  padding: 0 !important;
  margin: 1em !important;
  position: inherit !important;
}

@media screen and (min-width: 320px) and (max-width: 440px) and (max-height: 667px){
