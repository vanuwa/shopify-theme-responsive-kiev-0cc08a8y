.top_bar a:hover {
  color: {{ settings.top_bar_color_hover}};
}
.top_bar ul {
  padding:0;
  margin: 5px 0 0 0;
  border: 0;
  text-align: right;
}
.top_bar ul li {
  border-bottom: 0;
  border-top: 0;
  padding: 0 10px;
  position: relative;
  display: inline-block;
}

{% if settings.fix_top_bar %}
  .top_bar {
    position: fixed;
    z-index: 8000;
    width: 100%;
  }
  div.logo {
    padding-top: 55px !important;
  }
{% endif %}

#nav, #mobile_nav {
