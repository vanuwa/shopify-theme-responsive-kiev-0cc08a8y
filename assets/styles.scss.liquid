  text-align: {{ settings.menu_alignment }};

  {% if settings.nav_border == 'double' %}
    border-top: double {{ settings.border_color }};
    border-bottom: double {{ settings.border_color }};
  {% else %}
    border-top: 1px {{ settings.nav_border }} {{ settings.border_color }};
    border-bottom: 1px {{ settings.nav_border }} {{ settings.border_color }};
  {% endif %}
  margin: 20px auto 0;
  min-height: 35px;
  padding: 15px 0 8px 0;
}
#mobile_nav {
  display: none;
}
#mobile_nav ul, #mobile_nav ul ul {
  margin-left: 0;
}
#mobile_nav ul li, #mobile_nav ul li a {
  margin: 0;
}
#mobile_nav ul li a {
  padding: 0 !important;
  display: inline-block !important;
}
#mobile_nav ul :last-child {
  border: none;
  padding-bottom: 0;
}
.slicknav_arrow { padding-left: 5px; }
.slicknav_btn { padding-top: 2px !important; }
.slicknav_nav li { list-style: none; }
#mobile_nav {
  min-height: 30px;
  padding: 5px 0 0;
}
#nav ul {
    display: block;
    margin: 0 auto;
    text-align: {{ settings.menu_alignment }};
    border: none;
    padding: 0;
    margin: 0;
}
#nav ul li {
    display: inline-block;
    border-color: {{ settings.border_color }};
    border-top: 0;
    border-bottom: 0;
    border-right: 1px {{ settings.nav_separator }} {{ settings.border_color }};
    padding: 0 20px 0;
    position: relative;
    margin: 0;
}
{% if settings.nav_border == 'none' %}
  #nav ul li:first-child {
    padding-left: 0;
  }
{% endif %}
#nav ul li a, #nav ul li a:visited, #nav ul li a:active,
#mobile_nav ul li a, #mobile_nav ul li a:visited, #mobile_nav ul li a:active, a.slicknav_btn { 
    font-family: {{ settings.nav_font | replace: 'Google_', '' | replace: '+', ' ' }};
    font-weight: {{ settings.nav_weight }};      
    font-size: {{ settings.nav_font_size }}px; 
    text-transform: {{ settings.nav_font_style }};
    color: {{ settings.nav_color }};
